<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.icrisat.gdms.retrieve">
	<class name="RetrievalMarkers" table="marker_det">
  		<id name="gid" column="gid"></id>
  		<property name="germplasm_name"></property>
  		<property name="marker_name"></property>
  		<property name="marker_id"></property>
  		<property name="char_value"></property>
  		<property name="species"></property>
  		<property name="marker_type"></property>
  	</class>
  	
  	<class name="Markers" table="marker">
  		<id name="marker_id" column="marker_id"></id>  		
  		<property name="marker_name"></property>
  		<property name="crop"></property>
  		<property name="marker_type"></property>
  	</class>
  	
  </hibernate-mapping>
 